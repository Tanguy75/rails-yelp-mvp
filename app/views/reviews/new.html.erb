<div class="container">
  <h1>Create a new comment</h1>
  <%= simple_form_for [@restaurant, @review] do |f| %>
    <%= f.input :content %>
    <%= f.input :rating %>
    <%= f.submit "add a comment", class: "btn btn-primary" %>
  <% end %>
<p><%#=link_to "Back to list", restaurant_reviews_path %></p>
</div>
